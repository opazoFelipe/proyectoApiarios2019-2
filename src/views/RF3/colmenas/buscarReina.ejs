<%- include ('../../partials/header')  %>
<%- include ('../../partials/mensaje')  %>
<div id="mensaje"></div>

<script type="application/javascript" src="../javascript/RF3/buscarReina.js"></script>

<div class="container mt-5">
    <div class="row">
        <div class="col-md-6 offset-md-3">
            <div class="card">
                <div class="card-title text-center">
                    <h1 class="pt-2">Buscar Reina por ID</h1>
                </div>
                <div class="card-body">
                    <form action="#" onsubmit="buscarReinaPorID(this)">
                        <div class="form-group">
                            <label for="idReina">Ingrese ID</label>
                            <input type="number" name="idReina" id="" class="form-control">
                        </div>
                        <div class="form-group">
                            <input type="submit" value="Buscar ID" class="btn btn-primary">
                        </div>
                    </form>
                    <div id="divDatosEncontrados" style="display: none;">
                        <table class="table">
                            <thead class="thead-dark">
                              <tr>
                                <th scope="col">ID</th>
                                <th scope="col">Descripción</th>
                                <th scope="col">Raza</th>
                                <th scope="col">Acción</th>
                              </tr>
                            </thead>
                            <tbody id="bodyTablaReina">
                             
                            </tbody>
                          </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<%- include ('../../partials/footer')  %>
